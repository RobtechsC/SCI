	<queryString>
		<![CDATA[SELECT
     EVAL_incidencias."consecutivo" AS EVAL_incidencias_consecutivo,
     EVAL_incidencias."id_tipo" AS EVAL_incidencias_id_tipo,
     EVAL_incidencias."id_subtipo" AS EVAL_incidencias_id_subtipo,
     EVAL_incidencias."cod_chofer" AS EVAL_incidencias_cod_chofer,
     EVAL_incidencias."fecha" AS EVAL_incidencias_fecha,
     EVAL_incidencias."id_bus" AS EVAL_incidencias_id_bus,
     EVAL_incidencias."id_ruta" AS EVAL_incidencias_id_ruta,
     EVAL_incidencias."hora" AS EVAL_incidencias_hora,
     EVAL_incidencias."tiempo_perdido" AS EVAL_incidencias_tiempo_perdido,
     EVAL_incidencias."km" AS EVAL_incidencias_km,
     EVAL_incidencias."rendimiento" AS EVAL_incidencias_rendimiento,
     EVAL_incidencias."lugar" AS EVAL_incidencias_lugar,
     EVAL_incidencias."justificable" AS EVAL_incidencias_justificable,
     EVAL_incidencias."culpable" AS EVAL_incidencias_culpable,
     EVAL_incidencias."monto_exacto" AS EVAL_incidencias_monto_exacto,
     EVAL_incidencias."comentario" AS EVAL_incidencias_comentario,
     EVAL_anomalias."id_tipo" AS EVAL_anomalias_id_tipo,
     EVAL_anomalias."Descripcion" AS EVAL_anomalias_Descripcion,
     EVAL_anomalias."Peso_imagen" AS EVAL_anomalias_Peso_imagen,
     EVAL_anomalias."Peso_economico" AS EVAL_anomalias_Peso_economico,
     EVAL_anomalias."Peso_global" AS EVAL_anomalias_Peso_global,
     EVAL_subanomalia."Peso" AS EVAL_subanomalia_Peso,
     EVAL_subanomalia."Descripcion" AS EVAL_subanomalia_Descripcion,
     EVAL_subanomalia."id_subtipo" AS EVAL_subanomalia_id_subtipo,
     empleado."idEmpleado" AS empleado_idEmpleado,
     empleado."nombre" AS empleado_nombre,
     empleado."asignado" AS empleado_asignado
FROM
     "dbo"."EVAL_anomalias" EVAL_anomalias FULL OUTER JOIN "dbo"."EVAL_incidencias" EVAL_incidencias ON EVAL_anomalias."id_tipo" = EVAL_incidencias."id_tipo"
     INNER JOIN "dbo"."EVAL_subanomalia" EVAL_subanomalia ON EVAL_anomalias."id_tipo" = EVAL_subanomalia."id_tipo"
     AND EVAL_subanomalia."id_subtipo" = EVAL_incidencias."id_subtipo"
     INNER JOIN "dbo"."empleado" empleado ON EVAL_incidencias."cod_chofer" = empleado."idEmpleado"
ORDER BY
     EVAL_incidencias."cod_chofer" ASC,
     EVAL_incidencias."id_tipo" ASC,
     EVAL_incidencias."id_subtipo" ASC]]>
	</queryString>
